{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "theme": "Alabaster",
  "base_keymap": "JetBrains",
  "buffer_font_size": 14,
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_fallbacks": ["FiraCode Nerd Font"],
  "buffer_font_weight": 300,
  "buffer_line_height": "standard",
  "current_line_highlight": "none",
  "tabs": {
    "close_position": "left",
    "activate_on_close": "neighbour"
  },
  "gutter": {
    "folds": true,
    "code_actions": true,
    "line_numbers": false,
    "runnables": false
  },
  "ui_font_weight": 300,
  "ui_font_family": "FiraCode Nerd Font",
  "ui_font_size": 15,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "cursor_blink": true,
  "hour_format": "hour24",
  "preferred_line_length": 120,
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "selections_menu": false
  },
  "lsp": {
    "gopls": {
      "initialization_options": {
        "hints": {
          "compositeLiteralFields": true,
          "constantValues": true,
          "parameterNames": true
        }
      }
    }
  },
  "git": {
    "inline_blame": {
      "enabled": false,
      "delay_ms": 3000,
      "show_commit_summary": true
    }
  },
  "terminal": {
    "blinking": "on",
    "cursor_shape": "bar"
  },
  "scrollbar": {
    "show": "never"
  },
  "languages": {
    "Go": {
      "hard_tabs": true,
      "tab_size": 3
    }
  },
  "indent_guides": {
    "enabled": false
  },
  "project_panel": {
    "indent_guides": {
      "show": "never"
    },
    "git_status": true,
    "indent_size": 16
  },
  "outline_panel": {
    "indent_size": 16,
    "indent_guides": {
      "show": "never"
    }
  }
}
