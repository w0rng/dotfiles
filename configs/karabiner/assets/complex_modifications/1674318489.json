{
  "title": "Option + wasd = arrow",
  "rules": [
    {
      "description": "Option + wasd = arrow",
      "manipulators": [
        {
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}